{
	"collection": "@nestjs/schematics",
	"monorepo": true,
	"root": "apps/server",
	"sourceRoot": "apps/server/src",
	"compilerOptions": {
		"webpack": false,
		"tsConfigPath": "apps/server/tsconfig.app.json"
	},
	"projects": {
		"server": {
			"type": "application",
			"root": "apps/server",
			"entryFile": "main",
			"sourceRoot": "apps/server/src",
			"compilerOptions": {
				"assets": "**/*.yaml",
				"tsConfigPath": "apps/server/tsconfig.app.json"
			}
		}
	}
}
