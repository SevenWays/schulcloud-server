	"version": "0.2.0",
	"configurations": [
		{
			"name": "Attach to NestJS",
			"port": 9229,
			"request": "attach",
			"skipFiles": ["<node_internals>/**"],
			"type": "pwa-node",
			"restart": true
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch and Deubg NestJS via NPM",
			"runtimeExecutable": "npm",
			"runtimeArgs": ["run-script", "nest:start:debug"],
			"port": 9229,
			"skipFiles": ["<node_internals>/**"],
			"console": "integratedTerminal"
		}
	]
}
